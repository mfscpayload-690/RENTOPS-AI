# Example environment variables for RENTOPS-AI AI layer
# Copy to .env and fill in your real key (do NOT commit .env)
GROQ_API_KEY=__REPLACE_ME__
AI_ENABLE=true

# For heuristic-only testing of intent extraction, either remove GROQ_API_KEY
# or set AI_ENABLE=false. The system will still parse passengers, vehicle type,
# simple locations, and relative dates without calling the LLM.

# Enable DB logging of AI calls (writes to ai_query_log table) if true and AI enabled.
AI_LOG_DB_ENABLE=false
